{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\run",
            "args": [
                "changed-elements",
                "enable",
                "--imodel-id",
                "<imodel-id>",
                "--itwin-id",
                "<itwin-id>"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch readme",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\run",
            "args": [
                "readme"
            ]
        }
    ]
}